{
    "name": "typescript-development-extension-pack",
    "displayName": "TypeScript Development Extension Pack",
    "description": "This exension pack is a currated list of all the extension you may need for TypeScript development.",
    "version": "2.0.0",
    "scripts": {
        "vsce": "vsce",
        "vsce:package": "vsce package",
        "vsce:publish": "vsce publish",
        "vscode:publish": "npm run vsce:publish",
        "postversion": "git push && git push --tags && npm run vsce:package"
    },
    "engines": {
        "vscode": "^1.36.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/markusfalk/vscode-typescript-extension-pack"
    },
    "publisher": "MarkusFalk",
    "icon": "logo-typescript.png",
    "categories": [
        "Extension Packs"
    ],
    "extensionPack": [
        "mike-co.import-sorter",
        "ms-vscode.vscode-typescript-tslint-plugin",
        "markusfalk.javascript-development-extension-pack"
    ]
}
